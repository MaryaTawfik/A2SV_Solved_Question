if __name__ == '__main__':
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
    res=[]
    for i in range(x+1):
        for j in range(y+1):
            for k in range(z+1):
                row=[0,0,0]
                row[0]=i
                row[1]=j
                row[2]=k
                if sum(row)!=n:
                    res.append(row)
    print(res )            
    
            
